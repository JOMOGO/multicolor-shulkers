{
	"schemaVersion": 1,
	"id": "multicolor-shulkers",
	"version": "${version}",
	"name": "Multi-Color Shulker Boxes",
	"description": "Allows you to color shulker boxes with multiple colors (top and bottom)",
	"authors": [
		"JOMOGO"
	],
	"contact": {
		"sources": "https://github.com/JOMOGO/multicolor-shulkers"
	},
	"license": "MIT",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.multicolorshulkers.MultiColorShulkers"
		],
		"client": [
			"com.multicolorshulkers.client.MultiColorShulkersClient"
		]
	},
	"mixins": [
		{
			"config": "multicolor-shulkers.client.mixins.json",
			"environment": "client"
		}
	],
	"accessWidener": "multicolor-shulkers.accesswidener",
	"depends": {
		"fabricloader": ">=0.16.0",
		"minecraft": "~1.21.4",
		"java": ">=21",
		"fabric-api": "*"
	}
}
